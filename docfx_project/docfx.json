{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "SpriteAnimations.csproj"
          ],
          "src": "../../sprite-animations-development"
        }
      ],
      "dest": "api",
      "outputFormat": "mref",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "template": [
      "default",
      "templates/darkfx"
    ],
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "documentation/**.md",
          "documentation/**.yml",
          "documentation/animations-manager/**.yml",
          "documentation/animations-manager/**.md",
          "documentation/installation/**.yml",
          "documentation/installation/**.md",
          "documentation/sprite-animator/**.yml",
          "documentation/sprite-animator/**.md",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}